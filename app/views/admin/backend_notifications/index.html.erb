<h1>Notifications</h1>
<br />
<% @backend_notifications.each do |backend_notification| %>
  <div class="card">
    <div class="card-body d-flex justify-content-between">
      <span>
        <%= backend_notification.message %>
      </span>
      <%= link_to raw('<i class="fas fa-times text-danger"></i>'), dismiss_admin_backend_notification_path(backend_notification), method: :put %>
    </div>
  </div>
  <br />
<% end %>
